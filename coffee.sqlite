CREATE TABLE coffee (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    roast_level TEXT NOT NULL,
    ground_or_beans TEXT NOT NULL,
    taste_description TEXT,
    price REAL NOT NULL,
    package_volume REAL NOT NULL
);

